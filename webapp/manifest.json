{
  "sap.app": {
    "id": "agraebe.custom",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "ach": "CA-UI5-DOC"
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      viewName: "agraebe.custom.view.App",
      type: "XML",
      async: true,
      id: "app"
    },
    "dependencies": {
      "minUI5Version": "1.30",
      "libs": {
        "sap.m": {}
      }
    },
    "config": {
      "chartjsModel": "/model/data_chartjs.json",
      "c3jsModel": "/model/data_c3js.json"
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "agraebe.custom.i18n.i18n"
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "agraebe.custom",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [{
        "pattern": "",
        "name": "main",
        "target": "main"
      }],
      "targets": {
        "main": {
          "viewName": "view/Main",
          "viewLevel": 1
        }
      }
    },
    "resources": {
      "css": [{
        "uri": "css/c3.min.css"
      }, {
        "uri": "css/style.css"
      }],
      "js": [{
        "uri": "/control/Chart.min.js"
      }, {
        "uri": "/control/d3.min.js"
      }, {
        "uri": "/control/c3.min.js"
      }]
    }
  }
}
